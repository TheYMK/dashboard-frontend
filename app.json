{
	"name": "`DashboardByKaymKassaiFront`",
	"description": "Dashboard App by Kaym Kassai",
	"website": "https://kaymkassai.com/",
	"repository": "https://github.com/TheYMK/dashboard-frontend",
	"success_url": "/",
	"scripts": {
		"predeploy": "npm install && npm run build"
	},
	"env": {
		"NODE_ENV": {
			"description": "Environment [production/development]",
			"value": "production"
		},
		"VITE_API_URL": {
			"description": "API URL",
			"value": "https://dashboard-by-kaym-kassai-api.herokuapp.com/v1/api"
		}
	},
	"formation": {
		"web": {
			"quantity": 1
		}
	},
	"image": "heroku/nodejs",
	"addons": ["heroku-postgresql"],
	"buildpacks": [
		{
			"url": "heroku/nodejs"
		}
	]
}
